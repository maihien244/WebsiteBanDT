<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.1.3/TweenMax.min.js"></script>
<div class="super_container border-bottom" style=" overflow: visible;">
	
	<!-- Header -->
	
	<header class="header">
		<link rel="stylesheet" type="text/css" href="/css/header.css">

		<!-- Top Bar -->

		<div class="top_bar">
			<div class="container">
				<div class="row">
					<div class="col d-flex flex-row justify-content-around align-items-center">
						<div class="top_bar_contact_item"><div class="top_bar_icon"><img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918577/phone.png" alt=""></div>+91 9823 132 111</div>
						<div class="top_bar_contact_item"><div class="top_bar_icon"><img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918597/mail.png" alt=""></div><a href="mailto:maivanhien244@gmail.com">contact@bshop.com</a></div>
						<div class="top_bar_content ml-auto">
							<div class="top_bar_user">
								{{#if configHeader.userLogin}}
									<ul class="standard_dropdown main_nav_dropdown">
										<li>
											<a href="#">
												<div class="user_icon">
													<img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918647/user.svg" alt="">
												</div>
												{{configHeader.user.fullname}}
												<i class="fas fa-chevron-down"></i>
											</a>
											<ul>
												<li><a href="/private/infor">Personal Information</a></li>
												<li><a href="/logout">Log Out</a></li>
											</ul>
										</li>
									</ul>
								{{else}}
									<div><a href="/register">Register</a></div>
									<div><a href="/login">Sign in</a></div>
								{{/if}}
							</div>
						</div>
					</div>
				</div>
			</div>		
		</div>

		<!-- Header Main -->

		<div class="header_main">
			<div class="container" >
				<div class="row">

					<!-- Logo -->
					<div class="col-lg-2 col-sm-3 col-3 order-1">
						<div class="logo_container">
							<div class="logo"><a href="/">BSHOP</a></div>
						</div>
					</div>

					<!-- Search -->
					<div class="col-lg-6 col-12 order-lg-2 order-3 text-lg-left text-right" >
						<div class="header_search">
							<div class="header_search_content">
								<div class="header_search_form_container" style="position: relative !important;">
									<form action="#" class="header_search_form clearfix">
										<input type="search" required="required" id="search-input" class="header_search_input" placeholder="Search for products...">
										<button type="submit" class="header_search_button trans_300" value="Submit"><img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918770/search.png" alt=""></button>
									</form>
									<ul id="searchResults" class="list-group mt-2 d-none" >
										{{!-- <li class="list-group-item list-group-item-action">
											<div class="row h-100 text-center">
												<div class="col-3" style="object-fit: cover;">
													<img height="40" src="data:image/jpeg;base64," alt="">
												</div>
												<div class="col-9">
													<div class="row">
														<div class="col-6 text-left">
															Product name
														</div>
														<div class="col-6 text-center">
															<p>Price</p>
														</div>
													</div>
												</div>
											</div>
										</li> --}}
									</ul>
								</div>
							</div>
						</div>
						
					</div>
					<script>
						const input_search = document.getElementById('search-input')
						const searchResults = document.getElementById('searchResults')
						input_search.addEventListener('input', async function(event) {
							console.log(input_search.value)
							const data = (await axios.post('http://localhost:3000/search', {
								textSearch: input_search.value
							})).data
							{{!-- console.log(data) --}}

							if(data.type == 'success') {
								const products = data.data
								if(products.length > 0) {
									searchResults.innerHTML = ''
									products.forEach(product => {
										const li = document.createElement('li')
										li.classList.add('list-group-item', 'list-group-item-action')
										li.innerHTML = `
											<li class="list-group-item list-group-item-action">
											<div class="row h-100 text-center">
												<div class="col-3" style="object-fit: cover;">
													<img height="40" src="${product.img}" alt="">
												</div>
												<div class="col-9">
													<div class="row">
														<div class="col-6 text-left">
															<h5>${product.name}</h5>
														</div>
														<div class="col-6 text-center">
															<p>${product.price}$</p>
														</div>
													</div>
												</div>
											</div>
										</li>`
										searchResults.appendChild(li)
									})
									searchResults.classList.remove('d-none')
								} else {
									searchResults.classList.add('d-none')
								}
							} else {
								alert('Don\'t have any product match with your search')
							}
						})
					</script>
					<!-- Wishlist -->
					{{#if configHeader.userLogin}}
						<div class="col-lg-4 col-9 order-lg-3 order-2 text-lg-left text-right">
							<div class="wishlist_cart d-flex flex-row align-items-center justify-content-end">
								<div class="wishlist d-flex flex-row align-items-center justify-content-end">
									<div class="wishlist_icon"><img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918681/heart.png" alt=""></div>
									<div class="wishlist_content">
										<div class="wishlist_text"><a href="http://localhost:3000/private/wishlist">Wishlist</a></div>
										<div class="wishlist_count">{{configHeader.user.wishlist_count}}</div>
									</div>
								</div>

								<!-- Cart -->
								<div class="cart">
									<div class="cart_container d-flex flex-row align-items-center justify-content-end">
										<div class="cart_icon">
											<img src="https://res.cloudinary.com/dxfq3iotg/image/upload/v1560918704/cart.png" alt="">
											<div class="cart_count"><span>{{configHeader.user.cart_count}}</span></div>
										</div>
										<div class="cart_content">
											<div class="cart_text"><a href="http://localhost:3000/private/cart">Cart</a></div>
											<div class="cart_price">${{configHeader.user.cart_price}}</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					{{/if}}
				</div>
			</div>
		</div>
	</header>
	<div class="border-bottom" style="height: 100px"></div>
	</div>

</div>